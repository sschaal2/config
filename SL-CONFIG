setenv LAB_ROOT /Network/Servers/duerer/Volumes/duerer/clmc/prog
setenv PROG_ROOT /Network/Servers/duerer/Volumes/duerer/clmc/prog
 
if ( $MACHTYPE == "i386" && `uname -a | grep -c Darwin` > 0 ) then 
   setenv MACHTYPE i386mac 
endif 

if ( $MACHTYPE == "x86_64" && `uname -a | grep -c Darwin` > 0 ) then 
   setenv MACHTYPE x86_64mac 
endif 

if ( `uname -a | grep -c ipipe` > 0 ) then 
   setenv MACHTYPE $MACHTYPE"xeno" 
endif 

alias imake           "imake -I$LAB_ROOT/imake -TLAB_Imake.tmpl -D$MACHTYPE" 

